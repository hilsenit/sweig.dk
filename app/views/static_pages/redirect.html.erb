
<div class="row">
	<div class="image">
		<%= image_tag "baggrund-til-sweig-logo-skarpt", class: "sweig" %>
		<div id="countDown"></div>
	</div>
</div>

<script type="text/javascript">
	$(document).ready(function() {
		var newYear = new Date(); 
		newYear = new Date(newYear.getFullYear(), 8, 16, 14); 
		$('#countDown').countdown({until: newYear, timezone: +2}); 		
	});

</script>
