

		<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
			<% unless @published_works.empty? %>
			<h2>Udgivet</h2>
				<%= render partial: "user_works", 
				locals: {works: @published_works, arrow: "down"} %>
			<% end %>

			<% if !@draft_works.empty? %>
			<h2>Kladder</h2>
				<%= render partial: "user_works", 
				locals: {works: @draft_works, arrow: "up"} %>
			<% end %>
		</div>
	</div>
</div>


